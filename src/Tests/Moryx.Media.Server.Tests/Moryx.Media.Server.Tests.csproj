    <None Include="..\TestContent\test.png" Link="TestContent\test.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\TestContent\test.txt" Link="TestContent\test.txt">
