<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <OutputType>Library</OutputType>
    <DebugType>full</DebugType>
    <IsTestProject>true</IsTestProject>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="..\TestContent\test.jpg" Link="TestContent\test.jpg" />
    <Content Include="..\TestContent\test.png" Link="TestContent\test.png" />
