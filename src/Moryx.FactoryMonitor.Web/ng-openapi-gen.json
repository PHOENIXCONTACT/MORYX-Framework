{
  "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:5000/swagger/v1/swagger.json",
  "output": "src/app/api",
  "ignoreUnusedModels": true,
  "removeStaleFiles": true,
  "skipJsonSuffix": true,
  "includeTags": [
    "FactoryMonitor"
  ],
  "module": true,
  "services": true,
  "apiService": "",
  "enumStyle": "pascal",
  "enumArray": false,
  "promises": false
}