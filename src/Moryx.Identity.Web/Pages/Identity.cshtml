@page "{*_}"
@model Moryx.Identity.Web.Pages.IdentityModel
@using Moryx.Asp.Integration;
@using Moryx.Identity.Web.Properties
@using System.ComponentModel.DataAnnotations
@{
    ViewData["Title"] = "Identity";
    ViewData["BaseRef"] = "Identity/";
    Layout = "_Layout";
}
@attribute [WebModule("Identity", "person", Category = ModuleCategory.Settings),
  Display(ResourceType = typeof(Strings), Name = "Module_Title", Description = "Module_Description")]

<iframe style="width: 100%; height: 100%;" src="@Model.IdentityUrl" ></iframe>