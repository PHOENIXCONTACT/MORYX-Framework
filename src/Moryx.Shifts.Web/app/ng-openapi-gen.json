{
    "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
    "input": "http://localhost:5000/swagger/v1/swagger.json",
    "output": "src/app/api",
    "ignoreUnusedModels": true,
    "includeTags": [
        "ShiftManagement",
        "OperatorManagement",
        "OrderManagement",
        "SkillManagement"
    ],
    "defaultTag": "ShiftManagement",
    "module": true,
    "services": true,
    "apiService": "",
    "enumStyle": "pascal",
    "enumArray": false,
    "promises": false
}