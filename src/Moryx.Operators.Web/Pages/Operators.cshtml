@page "{*_}"
@using System.ComponentModel.DataAnnotations
@using Moryx.Asp.Integration
@using Moryx.Operators.Web
@using Moryx.Operators.Web.Localizations

@{
    ViewData["Title"] = Strings.Module_Title;
ViewData["BaseRef"] = "Operators";

Layout = "_Layout";
}

@attribute [WebModule("Operators", "engineering"),
Display(ResourceType = typeof(Strings), Name = "Module_Title", Description = "Module_Description")]
@section Styles {
<link rel="stylesheet" href="/_content/Moryx.Operators.Web/styles.css">
}
@section Scripts
    {
    <script src="/_content/Moryx.Operators.Web/runtime.js" type="module"></script>
    <script src="/_content/Moryx.Operators.Web/polyfills.js" type="module"></script>
    <script src="/_content/Moryx.Operators.Web/main.js" type="module"></script>
}

<app-root></app-root>