{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj",
            "**/*.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "**/Tests/**"
          ],
          "src": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "tutorials/**.md",
          "tutorials/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "resources/Phoenix_Contact_Logo.svg"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "globalMetadata": {
      "_appTitle": "MORYX Core",
      "_appFooter": "2021 PHOENIX CONTACT",
      "_appLogoPath": "resources/Phoenix_Contact_Logo.svg",
      "_enableNewTab": true,
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/PHOENIXCONTACT/MORYX-Core",
        "branch": "master"
      }
    }
  }
}